#include<stdint.h>
#include<stdio.h>

int main(){

	// initialization

	uint32_t studentAge[100];
	printf("Size of Array : %u \n", sizeof(studentAge));
	printf("Base address : %p \n", studentAge);

	// read and write
	uint8_t data[10] = {0xFF, 0xFF, 0xFE, 0xFD }; // remaining are 0
	printf("Data of array of index 6: %d" ,data[6]);

	return 0;
}
